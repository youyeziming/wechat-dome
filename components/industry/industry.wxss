/* package.customerFish/pages/customerFish/customerFish.wxss */
/* header */
.wrapperInside{
  height: 120rpx;
}
.customerFish {
  width: 100%;
}
/* industry */
.industryWrapper{
  margin-bottom: 10rpx;
}
.industry {
  width: 640rpx;
  /* height: 524rpx; */
  background: #FFFFFF;
  border: 1rpx solid #EEEEEE;
  border-radius: 10rpx;
  margin: auto;
  /* overflow: hidden; */
}

.industry .top {
  height: 88rpx;
  background: #FAFAFA;
  border-radius: 10rpx 10rpx 0rpx 0rpx;
  line-height: 88rpx;
  padding-left: 29rpx;
}

.industry .top image {
  width: 20rpx;
  height: 10rpx;
  transform: rotate(-270deg);
  position: relative;
  top: -7rpx;
}

.industry .top text {
  font-weight: bold;
  font-size: 30rpx;
  padding-left: 5rpx;
}

.industry .wrapper {
  display: flex;
  justify-content: flex-start;
  /* height: 0 !important; */
  border-bottom: 1rpx solid #EEEEEE;
}
.industry .wrapper:last-child{
  border-bottom: 0rpx;
}

.industry .wrapper .left {
  width: 169rpx;
  padding-top: 10rpx;
  padding-left: 15rpx;
  font-size: 30rpx;
  font-weight: 500;
  /* background-color: red; */
}

.industry .wrapper .right {
  flex: 1;
  padding-top: 6rpx !important;
  display: flex;
  flex-wrap: wrap;
}

