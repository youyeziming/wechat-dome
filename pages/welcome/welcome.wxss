/* pages/welcome/welcome.wxss */
.welcome{
  display: grid;
  place-items: center;
  width: 100%;
  height: 80vh;
font-size: 28rpx;
color: #333333;
font-weight: 500;
}
.welcome .wrapper{
  width: 600rpx;
  height: 130rpx;
  display: grid;
  place-items: center;
}
.welcome .wrapper view{
  padding-bottom: 10rpx;
}
.imgs{
  width: 108rpx;
  height: 108rpx;
  margin-bottom: 49rpx;
}
/* move */
.move1 {
  animation: myMove1 5s ease-in infinite alternate;
  -webkit-animation: myMove1 5s ease-in infinite alternate;
}


@keyframes myMove1 {
  from {transform: rotate(0deg);}
  to {transform: rotate(360deg);}
}

@-webkit-keyframes myMove1 {
  from {transform: rotate(0deg);}
  to {transform: rotate(360deg);}
}